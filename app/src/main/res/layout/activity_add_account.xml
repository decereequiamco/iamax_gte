<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:fancy="http://schemas.android.com/apk/res-auto"
    android:background="#424242"
    tools:context="com.gte_iamax.android.iamax_gte.AddAccount">
    <EditText
        android:id="@+id/et_accountID"
        style="@style/editText_style"
        android:layout_width="318dp"
        android:layout_height="60dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="54dp"
        android:ems="10"
        android:hint="Account ID"
        android:inputType="number"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.132"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <EditText
        android:id="@+id/et_firstName"
        style="@style/editText_style"
        android:layout_width="318dp"
        android:layout_height="60dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:hint="First Name"
        android:inputType="text"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.303"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <EditText
        android:id="@+id/et_lastName"
        style="@style/editText_style"
        android:layout_width="318dp"
        android:layout_height="60dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:hint="Last Name"
        android:inputType="text"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.48"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <EditText
        android:id="@+id/et_creditLimit"
        style="@style/editText_style"
        android:layout_width="318dp"
        android:layout_height="60dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="50dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:hint="Credit Limit"
        android:inputType="text"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.647"
        android:layout_marginStart="50dp"
        android:layout_marginEnd="8dp" />

    <EditText
        android:id="@+id/et_mobile"
        style="@style/editText_style"
        android:layout_width="318dp"
        android:layout_height="60dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="50dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:hint="Mobile Num"
        android:inputType="number"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.647"
        android:layout_marginStart="50dp"
        android:layout_marginEnd="8dp" />

    <me.omidh.liquidradiobutton.LiquidRadioButton
        android:layout_width="35dp"
        android:layout_height="26dp"
        android:padding="30dp"
        android:gravity="center"
        app:lrb_strokeWidth="1dp"
        app:lrb_explodeCount="3"
        app:lrb_outterPadding="8dp"
        app:lrb_innerCircleRadius="10dp"
        app:lrb_strokeRadius="10dp"
        app:lrb_inAnimDuration="500"
        app:lrb_outAnimDuration="500"
        app:lrb_checkedColor="@android:color/white"
        app:lrb_unCheckedColor="@android:color/white"
        android:text="Liquid Radio Button"
        android:id="@+id/liquidRadioButton2"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="435dp"
        app:layout_constraintLeft_toRightOf="@+id/textView17"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintVertical_bias="0.0"
        android:layout_marginEnd="8dp" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:text="Gender:"
        android:textColor="@android:color/background_light"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.089"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.739"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="57dp"
        android:layout_height="30dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:text="Male"
        android:textColor="#ffff"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.084"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.816"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />
    <TextView
        android:id="@+id/textView18"
        android:layout_width="87dp"
        android:layout_height="29dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:text="Female"
        android:textColor="#ffff"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.765"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.818"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />
    <me.omidh.liquidradiobutton.LiquidRadioButton
        android:layout_width="35dp"
        android:layout_height="26dp"
        android:padding="30dp"
        android:gravity="center"
        app:lrb_strokeWidth="1dp"
        app:lrb_explodeCount="3"
        app:lrb_outterPadding="8dp"
        app:lrb_innerCircleRadius="10dp"
        app:lrb_strokeRadius="10dp"
        app:lrb_inAnimDuration="500"
        app:lrb_outAnimDuration="500"
        app:lrb_checkedColor="@android:color/white"
        app:lrb_unCheckedColor="@android:color/white"
        android:text="Liquid Radio Button"
        android:id="@+id/liquidRadioButton"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.876"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="435dp"
        app:layout_constraintLeft_toRightOf="@+id/textView17"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintVertical_bias="0.02"
        android:layout_marginEnd="8dp" />

    <com.rilixtech.materialfancybutton.MaterialFancyButton
        android:id="@+id/clear"
        android:layout_width="93dp"
        android:layout_height="40dp"
        fancy:mfb_borderColor="#fca40f"
        fancy:mfb_borderWidth="3dp"
        fancy:mfb_defaultColor="#424242"
        fancy:mfb_focusColor="#fca40f"
        fancy:mfb_iconFont="fontawesome.ttf"
        fancy:mfb_iconPosition="left"
        fancy:mfb_radius="30dp"
        fancy:mfb_text="CLEAR ALL"
        fancy:mfb_textColor="#FFFFFF"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.247"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintVertical_bias="0.96"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <com.rilixtech.materialfancybutton.MaterialFancyButton
        android:id="@+id/add"
        android:layout_width="92dp"
        android:layout_height="41dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.681"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.962"
        fancy:mfb_borderColor="#fca40f"
        fancy:mfb_borderWidth="3dp"
        fancy:mfb_defaultColor="#FCA40F"
        fancy:mfb_focusColor="#424242"
        fancy:mfb_iconFont="fontawesome.ttf"
        fancy:mfb_iconPosition="left"
        fancy:mfb_radius="30dp"
        fancy:mfb_text="ADD"
        fancy:mfb_textColor="#424242"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />
</android.support.constraint.ConstraintLayout>
